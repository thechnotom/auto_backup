{
    "backups": {
        "src": "source",
        "dest_dir": "backups",
        "max_num": 5,
        "time": 300,
        "retry_time": 30,
        "immediately": true,
        "max_use_of_free_space": 0.5,
        "operations_package": null,
        "allow_skip": true
    },
    "logging": {
        "do_logging": true,
        "do_type": true,
        "do_location": true,
        "do_short_location": true,
        "do_timestamp": true,
        "do_type_missing_indicator": true,
        "strict_types": false,
        "type_error_handling": {
            "do_unknown_type_exception": false,
            "do_override_type_exception": true,
            "do_prohibited_type_exception": true
        },
        "console": {
            "enable": true
        },
        "file": {
            "enable": true,
            "clear": true,
            "output_filename": "logged_output.txt",
            "max_file_size": 1000000,
            "max_backups": 1
        },
        "types": {
            "info": true,
            "warning": true,
            "timer": true,
            "operation": true,
            "interaction": true,
            "error": true,
            "backup": true,
            "MESSAGE": false
        }
    }
}
